<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f4f4f4"
    tools:context="com.saas.saasuser.activity.AddTripApplyActivity">
    
    
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:text="公司名称(发票抬头)"
                android:textSize="16sp"
                android:padding="5dp"

                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:text="中仁车汇科技发展(深圳)有限公司"
                android:textSize="14sp"
                android:paddingLeft="5dp"
                android:paddingTop="15dp"
                android:id="@+id/name_company"
                android:paddingBottom="15dp"
                android:background="#ffffff"
                android:layout_height="wrap_content"/>


            <TextView
                android:layout_width="match_parent"
                android:text="企业税号(纳税识别号)"
                android:textSize="16sp"
                android:padding="5dp"

                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:text="9144030MA5DHPD1XG"
                android:textSize="14sp"
                android:paddingLeft="5dp"
                android:background="#ffffff"
                android:id="@+id/enterprise_id_number"
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_width="match_parent"
                android:text="开户银行(企业对公开户)"
                android:textSize="16sp"
                android:padding="5dp"
                android:id="@+id/bank0"
                android:visibility="invisible"

                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:text="中国工商银行"
                android:textSize="14sp"
                android:id="@+id/bank"
                android:visibility="invisible"
                android:paddingLeft="5dp"
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:background="#ffffff"
                android:layout_height="wrap_content"/>


            <TextView
                android:layout_width="match_parent"
                android:text="银行账号(不限类型)"
                android:textSize="16sp"
                android:padding="5dp"
                android:visibility="invisible"
                android:id="@+id/bank_account0"

                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:text="3332 1234 5678 99"
                android:textSize="14sp"
                android:id="@+id/bank_account"
                android:paddingLeft="5dp"
                android:visibility="invisible"
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:background="#ffffff"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_width="match_parent"
                android:text="联系电话(注册登记电话)"
                android:textSize="16sp"
                android:visibility="invisible"
                android:padding="5dp"

                android:id="@+id/contact_phone_number0"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:text="0755-88824112"
                android:textSize="14sp"
                android:paddingLeft="5dp"
                android:visibility="invisible"
                android:paddingTop="15dp"
                android:background="#ffffff"
                android:id="@+id/contact_phone_number"
                android:paddingBottom="15dp"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_width="match_parent"
                android:text="地址(工商注册地址)"
                android:textSize="16sp"
                android:padding="5dp"
                android:visibility="invisible"

                android:id="@+id/adress0"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="match_parent"
                android:text="广东省深圳市南山区软件产业基地"
                android:textSize="14sp"
                android:paddingLeft="5dp"
                android:id="@+id/adress"
                android:paddingTop="15dp"
                android:background="#ffffff"
                android:visibility="invisible"
                android:paddingBottom="15dp"
                android:layout_height="wrap_content"/>
            
            
        </LinearLayout>
        
    </ScrollView>

    












</LinearLayout>
